elif [[ "$avail_govs" == *"interactive"* ]]
then
write "${cpu}scaling_governor" interactive
write "${cpu}interactive/timer_rate" "$((SCHED_PERIOD_THROUGHPUT / 1000))"
write "${cpu}interactive/boost" "1"
write "${cpu}interactive/timer_slack" "-1"
